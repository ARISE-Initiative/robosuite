<mujoco model="yumi left girpper">
    <asset>
        <mesh name="gripper_base" file="meshes/yumi_gripper/base.stl" />
        <mesh name="gripper_coarse_base" file="meshes/yumi_gripper/coarse/base.stl" />
        <mesh name="gripper_finger" file="meshes/yumi_gripper/finger.stl" />
        <mesh name="gripper_coarse_finger" file="meshes/yumi_gripper/coarse/finger.stl" />
    </asset>
    <actuator>
      <motor name="gripper_l_actuator" joint="gripper_l_joint" ctrlrange="-50 50" ctrllimited="true" />
      <motor name="gripper_l_actuator_m" joint="gripper_l_joint_m" ctrlrange="-50 50" ctrllimited="true" />
    </actuator>
    <worldbody>
        <body name="left_gripper" pos="0 0 0" quat="1 0 0 0">
            <site name="ft_frame" pos="0 0 0" size="0.01 0.01 0.01" rgba="1 0 0 1" type="sphere" group="1"/>
            <inertial pos="0 0 0.17" quat="0.707107 0.707107 0 0" mass="0.3" diaginertia="0.09 0.07 0.05" />
            <!-- This site was added for visualization. -->
            <body name="eef" pos="0 0 0.097" quat="1 0 0 0">
                <site name="grip_site" pos="0 0 0" size="0.01 0.01 0.01" rgba="1 0 0 0.5" type="sphere" group="1"/>
                <site name="ee_x" pos="0.1 0 0" size="0.005 .1"  quat="0.707105  0 0.707108 0 " rgba="1 0 0 0" type="cylinder" group="1"/>
                <site name="ee_y" pos="0 0.1 0" size="0.005 .1" quat="0.707105 0.707108 0 0" rgba="0 1 0 0" type="cylinder" group="1"/>
                <site name="ee_z" pos="0 0 0.1" size="0.005 .1" quat="1 0 0 0" rgba="0 0 1 0" type="cylinder" group="1"/>
                <!-- This site was added for visualization. -->
                <site name="grip_site_cylinder" pos="0 0 0" size="0.005 10" rgba="0 1 0 0.3" type="cylinder" group="1"/>
            </body>

            <!-- Here starts the actual mesh definition -->
             <!--Start of gripper_l_base-->
            <body name="gripper_l_base" pos="0.0 0.0 0.007" quat="-1.0341155355510722e-13 -0.0 0.0 1.0">
            <inertial pos="0 0 0" quat="1.0 0.0 0.0 0.0" mass="0.2" diaginertia="0.0001 0.0001 0.0001" />
            <geom pos="0.0 0.0 0.0" quat="1.0 0.0 0.0 0.0" name="gripper_l_base_visual" group="1" contype="0" conaffinity="0" mesh="gripper_base" type="mesh" />
            <geom pos="0.0 0.0 0.0" quat="1.0 0.0 0.0 0.0" name="gripper_l_base_collision" group="0" mesh="gripper_coarse_base" type="mesh" />
            <!--Start of gripper_l_finger_r-->
            <body name="gripper_l_finger_r" pos="0.0 -0.0065 0.0837" quat="1.0 0.0 0.0 0.0">
            <inertial pos="0 0 0" quat="1.0 0.0 0.0 0.0" mass="0.01" diaginertia="0.000001 0.000001 0.000001" />
            <geom pos="0.0 0.0 0.0" quat="1.0 0.0 0.0 0.0" name="gripper_l_finger_r_visual" group="1" contype="0" conaffinity="0" mesh="gripper_finger" type="mesh" />
            <geom pos="0.0 0.0 0.0" quat="1.0 0.0 0.0 0.0" name="gripper_l_finger_r_collision" group="0" mesh="gripper_coarse_finger" type="mesh" />
            <joint name="gripper_l_joint" type="slide" frictionloss="0.1" damping="0.1" stiffness="0.1" axis="-1 0 0" range="0 0.025" />
            </body>
            <!--End of gripper_l_finger_r-->
            <!--Start of gripper_l_finger_l-->
            <body name="gripper_l_finger_l" pos="0.0 0.0065 0.0837" quat="-1.0341155355510722e-13 -0.0 0.0 1.0">
            <inertial pos="0 0 0" quat="1.0 0.0 0.0 0.0" mass="0.01" diaginertia="0.000001 0.000001 0.000001" />
            <geom pos="0.0 0.0 0.0" quat="1.0 0.0 0.0 0.0" name="gripper_l_finger_l_visual" group="1" contype="0" conaffinity="0" mesh="gripper_finger" type="mesh" />
            <geom pos="0.0 0.0 0.0" quat="1.0 0.0 0.0 0.0" name="gripper_l_finger_l_collision" group="0" mesh="gripper_coarse_finger" type="mesh" />
            <joint name="gripper_l_joint_m" type="slide" frictionloss="0.1" damping="0.1" stiffness="0.1" axis="-1 0 0" range="0 0.025" />
            </body>
            <!--End of gripper_l_finger_l-->
            </body>
            <!--End of gripper_l_base-->
        </body>
    </worldbody>
    <sensor>
        <force name="force_ee" site="ft_frame"/>
        <torque name="torque_ee" site="ft_frame"/>
    </sensor>
</mujoco>