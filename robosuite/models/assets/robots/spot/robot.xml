              <geom mesh="left_lower_leg_collision" class="spot_collision"/>
              <geom name="FL" class="spot_foot"/>
            </body>
          </body>
        </body>
      </body>
      <!-- End of FL Leg -->
      <!-- Start of RR Leg -->
      <body name="RR_leg">
        <site name="RR_center" pos="0 0 0" size="0.01" rgba="1 0.3 0.3 1" group="2"/>
        <body name="hr_hip" pos="-0.29785 -0.055 0">
          <inertial pos="0.00537435 -0.0128418 9.87523e-05" quat="1 1 -1 1" mass="1.68" diaginertia="0.00226544 0.00211474 0.00181296"/>
          <joint class="spot_collision" name="hr_hx_leg_joint" axis="1 0 0" range="-0.785398 0.785398"/>
          <geom material="BlackAbs" mesh="rear_right_hip" class="spot_visual"/>
