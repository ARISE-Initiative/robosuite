        </body>
      </body>
      <!-- End of RR Leg -->
      <!-- Start of RL Leg -->
      <body name="RL_leg">
        <site name="RL_center" pos="0 0 0" size="0.01" rgba="1 0.3 0.3 1" group="2"/>
