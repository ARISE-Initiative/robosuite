            <geom mesh="front_right_upper_leg_1" material="BlackAbs" class="spot_visual"/>
            <geom mesh="right_upper_leg_collision" class="spot_collision"/>
            <body name="fr_lleg" pos="0.025 0 -0.32">
              <inertial pos="0.00225794 4.6076e-05 -0.180082" quat="1 0 0 1" mass="0.35" diaginertia="0.00405032 0.00399902 0.00014934"/>
              <joint class="spot_collision" name="fr_kn_leg_joint" axis="0 1 0" range="-2.7929 -0.255648"/>
              <geom material="BlackAbs" mesh="front_right_lower_leg" class="spot_visual"/>
              <geom mesh="right_lower_leg_collision" class="spot_collision"/>
              <geom name="FR" class="spot_foot"/>
            </body>
          </body>
        </body>
      </body>
      <!-- End of FR Leg -->
      <!-- FL Leg -->
      <body name="FL_leg">
        <site name="FL_center" pos="0 0 0" size="0.01" rgba="1 0.3 0.3 1" group="2"/>
        <body name="fl_hip" pos="0.29785 0.055 0">
          <inertial pos="-0.00537435 0.0128418 9.87523e-05" quat="1 1 -1 1" mass="1.68" diaginertia="0.00226544 0.00211474 0.00181296"/>
