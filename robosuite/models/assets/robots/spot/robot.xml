            </body>
          </body>
        </body>
      </body>
      <!-- End of RL Leg -->
    </body>
  </worldbody>
  <contact>
    <exclude body1="body" body2="fl_uleg"/>
    <exclude body1="body" body2="fr_uleg"/>
    <exclude body1="body" body2="hl_uleg"/>
    <exclude body1="body" body2="hr_uleg"/>
  </contact>
  <actuator>
    <motor class="spot" name="fr_hx_leg" joint="fr_hx_leg_joint"/>
    <motor class="spot" name="fr_hy_leg" joint="fr_hy_leg_joint"/>
    <motor class="spot" name="fr_kn_leg" joint="fr_kn_leg_joint"/>
    <motor class="spot" name="fl_hx_leg" joint="fl_hx_leg_joint"/>
    <motor class="spot" name="fl_hy_leg" joint="fl_hy_leg_joint"/>
    <motor class="spot" name="fl_kn_leg" joint="fl_kn_leg_joint"/>
    <motor class="spot" name="hr_hx_leg" joint="hr_hx_leg_joint"/>
    <motor class="spot" name="hr_hy_leg" joint="hr_hy_leg_joint"/>
    <motor class="spot" name="hr_kn_leg" joint="hr_kn_leg_joint"/>
    <motor class="spot" name="hl_hx_leg" joint="hl_hx_leg_joint"/>
    <motor class="spot" name="hl_hy_leg" joint="hl_hy_leg_joint"/>
