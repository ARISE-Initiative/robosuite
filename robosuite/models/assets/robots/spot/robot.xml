    <mesh file="meshes/front_right_lower_leg.obj" name="front_right_lower_leg"/>
    <mesh file="meshes/rear_left_hip.obj" name="rear_left_hip"/>
    <mesh file="meshes/rear_left_upper_leg_0.obj" name="rear_left_upper_leg_0"/>
