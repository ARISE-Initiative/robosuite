<mujoco model="robot">
  <!-- <compiler angle="radian" assetdir="assets" autolimits="true"/> -->
  <compiler angle="radian" autolimits="true"/>

  <option timestep="0.004" integrator="implicitfast"/>

  <asset>
    <texture type="2d" name="robot_texture" file="assets/robot_texture.png"/>
    <!-- <texture type="2d" name="finger_base_texture" file="assets/finger_base_texture.png"/> -->
    <!-- <texture type="2d" name="finger_tip_texture" file="assets/finger_tip_texture.png"/> -->
    <material name="robot_mtl" texture="robot_texture"/>
    <!-- <material name="finger_base_mtl" texture="finger_base_texture"/> -->
    <!-- <material name="finger_tip_mtl" texture="finger_tip_texture"/> -->

    <!-- Collision meshes -->
    <mesh name="link_base_0_00" file="assets/link_base_0_00.stl"/>
    <mesh name="link_base_0_01" file="assets/link_base_0_01.stl"/>
    <mesh name="link_base_1_00" file="assets/link_base_1_00.stl"/>
    <mesh name="link_base_1_01" file="assets/link_base_1_01.stl"/>
    <mesh name="link_base_1_02" file="assets/link_base_1_02.stl"/>
    <mesh name="link_base_1_03" file="assets/link_base_1_03.stl"/>
    <mesh name="link_base_1_04" file="assets/link_base_1_04.stl"/>
    <mesh name="link_base_1_05" file="assets/link_base_1_05.stl"/>
    <mesh name="link_base_1_06" file="assets/link_base_1_06.stl"/>
    <mesh name="link_base_1_07" file="assets/link_base_1_07.stl"/>
    <mesh name="link_base_1_08" file="assets/link_base_1_08.stl"/>
    <mesh name="link_base_1_09" file="assets/link_base_1_09.stl"/>
    <mesh name="link_base_1_10" file="assets/link_base_1_10.stl"/>
    <mesh name="link_base_1_11" file="assets/link_base_1_11.stl"/>
    <mesh name="link_base_1_12" file="assets/link_base_1_12.stl"/>
    <mesh name="link_base_1_13" file="assets/link_base_1_13.stl"/>
    <mesh name="link_base_1_14" file="assets/link_base_1_14.stl"/>
    <mesh name="link_base_1_15" file="assets/link_base_1_15.stl"/>
    <mesh name="link_base_1_16" file="assets/link_base_1_16.stl"/>
    <mesh name="link_base_1_17" file="assets/link_base_1_17.stl"/>
    <mesh name="link_base_1_18" file="assets/link_base_1_18.stl"/>
    <mesh name="link_base_1_19" file="assets/link_base_1_19.stl"/>
    <mesh name="link_torso_00" file="assets/link_torso_00.stl"/>
    <mesh name="link_torso_01" file="assets/link_torso_01.stl"/>
    <mesh name="link_shoulder" file="assets/link_shoulder.stl"/>
    <mesh name="link_bicep" file="assets/link_bicep.stl"/>
    <mesh name="link_elbow" file="assets/link_elbow.stl"/>
    <mesh name="link_forearm" file="assets/link_forearm.stl"/>
    <mesh name="link_wrist" file="assets/link_wrist.stl"/>
    <!-- <mesh name="link_gripper" file="assets/link_gripper.stl"/> -->
    <mesh name="link_head_pan" file="assets/link_head_pan.stl"/>
    <mesh name="link_head_tilt" file="assets/link_head_tilt.stl"/>
    <!-- <mesh name="link_finger_base" file="assets/link_finger_base.stl"/> -->
    <!-- <mesh name="link_finger_tip" file="assets/link_finger_tip.stl"/> -->

    <!-- Visual meshes -->
    <mesh name="link_base_v" class="visual" file="assets/link_base_v.obj"/>
    <mesh name="link_torso_v" class="visual" file="assets/link_torso_v.obj"/>
    <mesh name="link_shoulder_v" class="visual" file="assets/link_shoulder_v.obj"/>
    <mesh name="link_bicep_v" class="visual" file="assets/link_bicep_v.obj"/>
    <mesh name="link_elbow_v" class="visual" file="assets/link_elbow_v.obj"/>
    <mesh name="link_forearm_v" class="visual" file="assets/link_forearm_v.obj"/>
    <mesh name="link_wrist_v" class="visual" file="assets/link_wrist_v.obj"/>
    <mesh name="link_wheel_v" class="visual" file="assets/link_wheel_v.obj"/>
    <mesh name="link_head_pan_v" class="visual" file="assets/link_head_pan_v.obj"/>
    <mesh name="link_head_tilt_v" class="visual" file="assets/link_head_tilt_v.obj"/>
  </asset>

<!-- TODO: Temporary solution, not sure if this affects the physics -->
  <default>
    <default class="robot">
      <joint damping="10.0" frictionloss="1" armature=".1"/>
    </default>

    <default class="visual">
      <geom type="mesh" contype="0" conaffinity="0" group="1" material="robot_mtl"/>
    </default>
      
    <default class="collision">
      <geom type="mesh" rgba="1 1 1 1" group="0" contype="1" conaffinity="1"/>
    </default>
        
    <default class="finger_base">
      <geom type="capsule" rgba="1 1 1 1" size="0.015 0.01" quat="1 0 1 0" mass="0.0133245" pos="0 -0.005 0.03"/>
    </default>
        
    <default class="finger_tip">
      <geom type="capsule" rgba="1 1 1 1" size="0.01 0.008" quat="1 0 1 0" mass="0.0161862"/>
    </default>
  </default>
      

  <worldbody>
    <body name="base">
      <body name="base_link" pos="0 0 0.065">
        <!-- geom for base -->
        <geom class="visual" mesh="link_base_v"/>
        <geom class="collision" mesh="link_base_0_00"/>
        <geom class="collision" mesh="link_base_0_01"/>
        <geom class="collision" mesh="link_base_1_00"/>
        <geom class="collision" mesh="link_base_1_01"/>
        <geom class="collision" mesh="link_base_1_02"/>
        <geom class="collision" mesh="link_base_1_03"/>
        <geom class="collision" mesh="link_base_1_04"/>
        <geom class="collision" mesh="link_base_1_05"/>
        <geom class="collision" mesh="link_base_1_06"/>
        <geom class="collision" mesh="link_base_1_07"/>
        <geom class="collision" mesh="link_base_1_08"/>
        <geom class="collision" mesh="link_base_1_09"/>
        <geom class="collision" mesh="link_base_1_10"/>
        <geom class="collision" mesh="link_base_1_11"/>
        <geom class="collision" mesh="link_base_1_12"/>
        <geom class="collision" mesh="link_base_1_13"/>
        <geom class="collision" mesh="link_base_1_14"/>
        <geom class="collision" mesh="link_base_1_15"/>
        <geom class="collision" mesh="link_base_1_16"/>
        <geom class="collision" mesh="link_base_1_17"/>
        <geom class="collision" mesh="link_base_1_18"/>
        <geom class="collision" mesh="link_base_1_19"/>
        <!-- geom for wheels -->
        <geom class="visual" pos="0 0.18283 0" mesh="link_wheel_v"/>
        <geom class="visual" pos="0 -0.18283 0" mesh="link_wheel_v"/>
        <geom class="collision" pos="0 0.18283 0" size="0.06035 0.00925" quat="1 1 0 0" type="cylinder"/>
        <geom class="collision" pos="0 0.18283 0" size="0.06435 5e-06" quat="1 1 0 0" type="cylinder"/>
        <geom class="collision" pos="0 -0.18283 0" size="0.06035 0.00925" quat="1 1 0 0" type="cylinder"/>
        <geom class="collision" pos="0 -0.18283 0" size="0.06435 5e-06" quat="1 1 0 0" type="cylinder"/>
        <geom class="visual" pos="0 0 1.16902"  mesh="link_head_pan_v"/>
        <geom class="collision" pos="0 0 1.16902" mesh="link_head_pan"/>
        <geom class="visual" pos="0.07181 0 1.2875" mesh="link_head_tilt_v"/>
        <geom class="collision" pos="0.07181 0 1.2875" mesh="link_head_tilt"/>
        <!-- arm -->
        <body name="link_0" pos="0 0 0.5945">
          <site name="right_center" pos="0 0 0" size="0.01" rgba="1 0.3 0.3 1" group="2"/>
          <inertial pos="0 0 0.108398" diaginertia="0.0184592 0.0154173 0.0102031" mass="2.46883"/>
          <joint name="joint_link_0" axis="0 0 1" range="-4.49 1.35"/>
          <geom class="visual" mesh="link_torso_v"/>
          <geom class="collision" mesh="link_torso_00"/>
          <geom class="collision" mesh="link_torso_01"/>
          <body name="link_shoulder" pos="0 -0.167289 0.113" gravcomp="1">
            <inertial pos="0.00108994 0.00565406 0.103479" mass="2.08054" diaginertia="0.0162136 0.0158994 0.00367842"/>
            <joint name="joint_shoulder" axis="0 1 0" range="-2.66 3.18"/>
            <geom class="visual" mesh="link_shoulder_v"/>
            <geom class="collision" mesh="link_shoulder" />
            <body name="link_bicep" pos="0 0.002289 0.215029" gravcomp="1">
              <inertial pos="9.39565e-05 -0.0191308 0.130972" mass="1.3989" diaginertia="0.00828463 0.00826766 0.00210613"/>
              <joint name="joint_bicep" axis="0 0 1" range="-2.13 3.71"/>
              <geom class="visual" mesh="link_bicep_v"/>
              <geom class="collision" mesh="link_bicep"/>
              <body name="link_elbow" pos="1.9206e-12 -0.064 0.185036" gravcomp="1">
                <joint name="joint_elbow" axis="0 1 0" range="-2.05 3.79"/>
                <geom class="visual" mesh="link_elbow_v"/>
                <geom class="collision" mesh="link_elbow" mass="0.788999"/>
                <body name="link_forearm" pos="-1.92032e-12 -0.101 0.0914359" gravcomp="1">
                  <joint name="joint_forearm" axis="0 0 1" range="-2.92 2.92"/>
                  <geom class="visual" mesh="link_forearm_v"/>
                  <geom class="collision" mesh="link_forearm" mass="0.819369"/>
                  <body name="link_wrist" pos="0 -0.00604 0.2735" gravcomp="1">
                    <site name="wrist" pos="0 0 0.14"/>
                    <joint name="joint_wrist" axis="0 1 0" range="-1.79 1.79"/>
                    <geom class="visual" mesh="link_wrist_v"/>
                    <geom class="collision" mesh="link_wrist" mass="0.543658"/>

                    <body name="right_hand" pos="0 0.0060384 0.10911" gravcomp="1"> <!-- prev named link_gripper-->
                      <!-- <site name="gripper" pos="0 0 0.13" group="5"/> -->
                      <inertial pos="-3.20302e-05 0.000371384 0.0387883" quat="0.997934 0.0574504 -0.0278948 -0.00699571" mass="0.41474" diaginertia="0.000327698 0.000308611 0.000272392"/>
                      
                      <joint name="joint_gripper" axis="0 0 1" range="-4.49 1.35"/>

                    </body>  <!-- gripper -->

                  </body>  <!-- wrist -->
                </body>  <!-- forearm -->
              </body>  <!-- elbow -->
            </body>  <!-- bicep -->
          </body>  <!-- shoulder -->
        </body>  <!-- torso -->
      </body>
</body>
  </worldbody>

   <actuator>
     <motor joint="joint_link_0" ctrlrange="-100 100" ctrllimited="true" name="torq_j1"/>
     <motor joint="joint_shoulder" ctrlrange="-100 100" ctrllimited="true" name="torq_j2"/>
     <motor joint="joint_bicep" ctrlrange="-100 100" ctrllimited="true" name="torq_j3"/>
     <motor joint="joint_elbow" ctrlrange="-100 100" ctrllimited="true" name="torq_j4"/>
     <motor joint="joint_forearm" ctrlrange="-100 100" ctrllimited="true" name="torq_j5"/>
     <motor joint="joint_wrist" ctrlrange="-100 100" ctrllimited="true" name="torq_j6"/>
     <motor joint="joint_gripper" ctrlrange="-100 100" ctrllimited="true" name="torq_j7"/>
   </actuator>
</mujoco>
